

public class PrintdecPattern {
    public static void main(String[] args) {
        int n =5;
        String output = generatePattern(n);
        System.out.println(output);
    }

    public static String generatePattern(int n) {
        String output = " ";
        for(int i=n;i>=1;i--){
            output += i;
            for(int j=1;j<=(n-i);j++){
                output +=i;
            }
        }
        return output;
    }
}
